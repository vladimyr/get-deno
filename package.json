{
  "name": "get-deno",
  "version": "1.2.6",
  "description": "deno installation script for npx",
  "keywords": [
    "deno",
    "download",
    "install",
    "installer"
  ],
  "homepage": "https://github.com/vladimyr/get-deno",
  "bugs": {
    "url": "https://github.com/vladimyr/get-deno/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vladimyr/get-deno.git"
  },
  "license": "MIT",
  "author": {
    "name": "Dario Vladovic",
    "email": "d.vladimyr+dev@gmail.com",
    "url": "https://github.com/vladimyr"
  },
  "bin": "cli.compact.js",
  "files": [
    "cli.compact.js"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "np",
    "test": "tape test.js | tap-spec"
  },
  "config": {
    "artifacts": {
      "win32": [
        "deno_win_x64.zip",
        "deno-x86_64-pc-windows-msvc.zip"
      ],
      "darwin": [
        "deno_osx_x64.gz",
        "deno-x86_64-apple-darwin.zip"
      ],
      "linux": [
        "deno_linux_x64.gz",
        "deno-x86_64-unknown-linux-gnu.zip"
      ]
    },
    "repo": "denoland/deno",
    "umask": 755
  },
  "dependencies": {},
  "devDependencies": {
    "@vladimyr/eslint-config": "^1.1.1",
    "debug": "^4.1.1",
    "eslint": "^6.3.0",
