{
  "name": "get-deno",
  "version": "1.2.3",
  "description": "deno installation script for npx",
  "bin": "cli.compact.js",
  "files": [
    "cli.compact.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "tape test.js | tap-spec",
    "build": "rollup -c rollup.config.js",
    "prepublishOnly": "npm run build",
    "release": "np"
  },
  "config": {
    "repo": "denoland/deno",
    "artifacts": {
      "win32": "deno_win_x64.zip",
      "darwin": "deno_osx_x64.gz",
      "linux": "deno_linux_x64.gz"
    },
    "umask": 755
  },
  "keywords": [
    "deno",
    "download",
    "install",
    "installer"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/vladimyr/get-deno.git"
  },
  "author": {
    "email": "d.vladimyr+dev@gmail.com",
    "name": "Dario Vladovic",
    "url": "https://github.com/vladimyr"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vladimyr/get-deno/issues"
  },
  "homepage": "https://github.com/vladimyr/get-deno",
  "dependencies": {},
  "devDependencies": {
    "debug": "^4.1.1",
    "eslint": "^5.12.0",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "exit-hook": "^2.0.0",
    "gauge": "^2.7.4",
    "gh-got": "^8.1.0",
    "github-parse-link": "^1.1.1",
    "kleur": "^3.0.1",
    "make-dir": "^3.0.0",
    "minimist": "^1.2.0",
    "minimist-options": "^4.0.0",
    "mississippi": "^4.0.0",
    "ms": "^2.1.1",
    "ora": "^3.0.0",
    "p-do-whilst": "^1.0.0",
    "pretty-bytes": "^5.1.0",
    "rollup": "^1.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.0",
