    "mississippi": "^4.0.0",
    "ms": "^2.1.2",
    "ora": "^4.0.0",
    "p-do-whilst": "^1.1.0",
    "pretty-bytes": "^5.3.0",
    "rollup": "^2.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postprocess": "^1.0.2",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-visualizer": "^4.0.0",
    "semver": "^7.0.0",
    "shell-profile": "^1.0.3",
